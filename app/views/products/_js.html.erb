<script type="text/javascript">
  $(function(){
    // 锚点
    var thisId = window.location.hash;
    if (thisId != ''){
      $(thisId).click();
    }

    $.ajax({
      type: "get",
      dataType: "json",
      url: "/check_login",
      data: "pids=" + $("#product_id").val(),
      success: function(data) {
        if(data.success){
          $.each(data.rs, function(i, d){
            $("#bid_price_li").html(d.bid_price).show();
            $("#market_price_li").html(d.market_price).show();
          });
        }else{
          $("#login_tip").show();
        }
      }
    })

  });
</script>